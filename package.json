{
  "name": "job-log",
  "private": true,
  "workspaces": [
    "web",
    "shared",
    "app"
  ],
  "scripts": {
    "build:web": "npm -w web run build",
    "build:shared": "npm -w shared run build",
    "build:app": "npm -w app run build",
    "build": "npm run build:shared && npm run build:web",
    "dev:web": "npm -w web run dev",
    "lint:web": "npm -w web run lint",
    "typecheck": "npm -w shared run typecheck && npm -w web run typecheck"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
